{
  "name": "cinaries",
  "private": true,
  "license": "GPL-2.0-only",
  "description": "Minimal container images that package selected binaries from OSS projects to cut download size, build time and carbon footprint.",
  "author": {
    "name": "Mate NakiÄ‡",
    "url": "https://github.com/covik"
  },
  "keywords": [
    "containers",
    "docker",
    "podman",
    "kubernetes",
    "oci",
    "binaries",
    "scratch",
    "minimal",
    "sustainability"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/covik/cinaries.git"
  },
  "bugs": {
    "url": "https://github.com/covik/cinaries/issues"
  },
  "homepage": "https://github.com/covik/cinaries#readme",
  "scripts": {
    "build": "turbo run build --summarize --log-order=grouped --ui=stream --output-logs=errors-only",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,mts,js,mjs,md,json}\"",
    "format:check": "prettier --check \"**/*.{ts,mts,js,mjs,md,json}\" --log-level=warn",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@openai/codex": "^0.34.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.16.0+sha512.8066e7b034217b700a9a4dbb3a005061d641ba130a89915213a10b3ca4919c19c037bec8066afdc559b89635fdb806b16ea673f2468fbb28aabfa13c53e3f769",
  "engines": {
    "node": ">= 24"
  }
}
